    <owl:NamedIndividual rdf:about="http://dcu.gr/ontologies/scholarlyontology/instances/ActivityType-Validating">
        <rdf:type rdf:resource="http://dcu.gr/ontologies/scholarlyontology/ActivityType"/>
        <definition>Validating refers to the Activity Type of verifying that something meets certain specified criteria.</definition>
        <hasBroaderTerm rdf:resource="http://dcu.gr/ontologies/scholarlyontology/instances/ActivityType-Analyzing"/>
        <prefLabel>4.2.1 Validating</prefLabel>
        <rdfs:label>Validating</rdfs:label>
