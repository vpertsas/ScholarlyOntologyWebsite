    <!-- http://dcu.gr/ontologies/scholarlyontology/instances/ActivityType-Versioning -->

    <owl:NamedIndividual rdf:about="http://dcu.gr/ontologies/scholarlyontology/instances/ActivityType-Versioning">
        <rdf:type rdf:resource="http://dcu.gr/ontologies/scholarlyontology/ActivityType"/>
        <definition>Versioning refers to the Activity Type of management and control of features and changes made to software throughout the life cycle of an ICT project.</definition>
        <hasBroaderTerm rdf:resource="http://dcu.gr/ontologies/scholarlyontology/instances/ActivityType-Preserving"/>
        <prefLabel>4.5.3 Versioning</prefLabel>
        <rdfs:label>Versioning</rdfs:label>
