        <rdf:type rdf:resource="http://dcu.gr/ontologies/scholarlyontology/ActivityType"/>
        <definition>Tracking refers to the Activity Type of following systematically and methodically something in order to note its course.</definition>
        <hasBroaderTerm rdf:resource="http://dcu.gr/ontologies/scholarlyontology/instances/ActivityType-Seeking"/>
        <prefLabel>2.8 Tracking</prefLabel>
        <rdfs:label>Tracking</rdfs:label>
